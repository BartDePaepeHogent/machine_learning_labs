   "source": [
    "n_samples = 54\n",
    "species_array = [49, 5]\n",
    "sum = 0\n",
    "for species in species_array:\n",
    "    ratio = species / n_samples\n",
    "    sqr = ratio ** 2\n",
    "    sum += sqr\n",
    "gini = 1 - sum\n",
    "print(f\"gini: {gini}\")"
